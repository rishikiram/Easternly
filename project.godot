[PluginSettings]

gdinv/LoadOnReady=true
gdinv/ItemJsonPaths=PoolStringArray( "res://resources/item data/" )

[application]

config/name="Easternly"
run/main_scene="res://scenes/StartMenu.tscn"
config/icon="res://Square_screenshot.png"

[autoload]

Global="*res://scripts/Global.gd"
GameData="*res://scripts/Singletons/GameData.gd"
PlayerVariables="res://scripts/PlayerVariables.gd"
GDInv_ItemDB="*res://addons/gdinv/GDInv_ItemDB.gd"
QuestDB="res://scripts/Singletons/Quest_DB.gd"
InventoryData="*res://scripts/Singletons/InventoryData.gd"
AudioManager="*res://scenes/AudioManager.tscn"

[debug]

gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false

[display]

mouse_cursor/custom_image="res://art/GUI/Cursor_Compass.png"
mouse_cursor/custom_image_hotspot=Vector2( 3, 0 )

[editor_plugins]

enabled=PoolStringArray( "dialogue_tree", "gdinv" )

